<!DOCTYPE html>
<html>
<head>
  <title>Jeffbook</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% flash.each do |key, val| %>
		<%= "#{key.to_s.upcase}! #{val}" %>
	<% end %>
	<br>
	<%= render 'shared/header' %>
	<br>
	<%= yield %>
	<br>
	<%= render 'shared/footer' %>
</body>
</html>
